<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary x:Class="$safeprojectname$.Resources.Dictionaries.Converters"
                    xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:LocalConverters="clr-namespace:$safeprojectname$.Converters"
                    xmlns:SharpnadoConverters="clr-namespace:Sharpnado.Presentation.Forms.Converters;assembly=Sharpnado.Presentation.Forms">

    <SharpnadoConverters:IsNotNullConverter x:Key="IsNotNullConverter" />
    <SharpnadoConverters:IsNullConverter x:Key="IsNullConverter" />
    <LocalConverters:WithoutExtensionConverter x:Key="WithoutExtensionConverter" />
    <LocalConverters:IntEnumConverter x:Key="IntEnumConverter" />
    <LocalConverters:NullToFalseConverter x:Key="NullToFalseConverter" />
    <LocalConverters:InvertBoolConverter x:Key="InvertBoolConverter" />

</ResourceDictionary>